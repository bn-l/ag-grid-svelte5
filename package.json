{
	"name": "ag-grid-svelte5-extended",
	"version": "0.0.12",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"bumpp": "npx bumpp",
		"publish-ci": "tsx scripts/publish-ci.ts",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prebumpp": "tsx scripts/updateInitialGridOptionsSet.ts",
		"test:unit": "vitest",
		"run:file": "tsx",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"license": "MIT",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bn-l/ag-grid-svelte5-extended"
	},
	"homepage": "https://bn-l.github.io/ag-grid-svelte5-extended/",
	"devDependencies": {
		"@antfu/eslint-config": "^3.11.2",
		"@faker-js/faker": "^9.2.0",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/node": "^22.10.1",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"publint": "^0.2.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.11",
		"vitest": "^2.0.4",
		"zx": "^8.2.4",
		"cookie": ">=0.7.0"
	},
	"dependencies": {
		"@ag-grid-community/client-side-row-model": "^32.3.3",
		"@ag-grid-community/core": "^32.3.3",
		"@ag-grid-community/theming": "^32.3.3",
		"svelte": "^5.0.0"
	},
	"overrides": {
		"svelte-eslint-parser": "^0.43.0",
		"cookie": ">=0.7.0"
	},
	"description": "AG-Grid wrapper component for Svelte 5 (runes) with support for Svelte components as cell renderers, reactive data updates, and enhanced performance",
	"keywords": [
		"svelte",
		"svelte5",
		"svelte-5",
		"sveltekit",
		"svelte-kit",
		"5",
		"runes",
		"ag-grid",
		"datagrid",
		"grid",
		"table",
		"data-table",
		"cell-renderer",
		"typescript",
		"reactive",
		"component",
		"ui",
		"datatable"
	]
}
